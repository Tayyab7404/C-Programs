#include <stdio.h>
int fact(int n)
{
    int i,fact=1;
    for(i=n;i>1;i--)
        fact = fact*i;
    return fact;
}

int main()
{
	int n,m,rem,sum=0;
	printf("Enter a number: ");
	scanf("%d",&n);
	m = n;
    while(n>0)
    {
        rem = n%10;
        sum += fact(rem);
        n = n/10;
    }
	if (sum == m)
		printf("%d is a Strong number\n",m);
	else
		printf("%d is not a Strong number\n",m);
}
